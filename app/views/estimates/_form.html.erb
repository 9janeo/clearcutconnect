<h2>Create an estimate</h2>

<div class="form-group">
	<%= form_for(@estimate) do |f| %>
		<%= f.label :owner %>
		<%= f.text_field :owner %><br />
		<%= f.label :email %>
		<%= f.text_field :email %><br />
		<%= f.label :phone %>
		<%= f.text_field :phone %><br />
		<%= f.label :notes %>
		<%= f.text_field :notes %><br />
		<%= f.label :types %>
		<%= f.check_box :type, {multiple: true}, 1, nil %><br />
	<% end %>
</div>