<%= form_for @order, :html => {:multipart => true} do |form| %>
  <% if order.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(order.errors.count, "error") %> prohibited this keyword from being saved:</h2>

      <ul>
      <% order.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <!-- (:ordername, :ordernumber, :complete, :servicelist, :created_at, :updated_at #Needs a type param -->
  <div class="form-group">
    <%= form.label :ordername %>
    <%= form.text_field :ordername, id: :ordername %>
  </div>

  <div class="form-group">
    <%= form.label :ordernumber %>
    <%= form.text_field :ordernumber, id: :ordernumber %>
  </div>

  <div class="form-group">
    <%= form.label :complete %>
    <%= form.text_field :complete, id: :complete %>
  </div>

  <div class="form-group">
    <%= form.label :servicelist %>
    <%= form.text_field :servicelist, id: :servicelist %>
  </div>

  <div class="form-group">
    <%= form.submit class: "btn btn-primary" %>
  </div>
<% end %>